<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.naeunminchocofarm.ncf_api.temperature.mapper.SoilMapper">
    <!--라즈베리에서 받아오는 자료가 들어가는 쿼리-->
    <insert id="insert">
        INSERT INTO soil_moisture_values (soil_moisture_value, measured_at)
        VALUES (#{soil_moisture_value}, #{measuredAt})
    </insert>

    <select id="getSoilMoistureValue" resultType="com.naeunminchocofarm.ncf_api.temperature.entity.AirTemperature">

    </select>

</mapper>